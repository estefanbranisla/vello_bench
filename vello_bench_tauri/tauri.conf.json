{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Vello Benchmark Suite",
  "version": "0.1.0",
  "identifier": "com.vello.bench",
  "build": {
    "frontendDist": "ui"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Vello Benchmark Suite",
        "width": 1400,
        "height": 900,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "category": "DeveloperTool",
    "targets": "all"
  }
}
